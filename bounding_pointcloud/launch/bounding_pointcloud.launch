<?xml version="1.0"?>
<launch>
    <arg name="rvizconfig" default="$(find bounding_pointcloud)/rviz/rviz.rviz"/> 

    <!-- darknet_ros -->
    <include file="$(find bounding_pointcloud)/launch/darknet_ros.launch">
    </include>

    <!--orb_slam2 -->
    <include file="$(find bounding_pointcloud)/launch/orb_slam2_mydataset.launch">
    </include>

    <!--octomap -->
    <include file="$(find bounding_pointcloud)/launch/octomap_mapping_orbslam_tum.launch">
    </include>


    <!-- nodelet -->
    <include file="$(find bounding_pointcloud)/launch/nodelet.launch">
    </include>

    <node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" required="true"/>

</launch>
